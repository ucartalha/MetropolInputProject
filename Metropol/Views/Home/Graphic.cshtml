<div class="row row-sm">
    <div class="col-sm-8 col-md-6 col-xl-4">
        <div class="az-content-label az-content-label-sm mg-b-15">Ofis Verileri</div>
        <div class="ht-200 ht-lg-250"><canvas id="chartBar1"></canvas></div>
        <div id="chart-tooltip"></div>
    </div>
    <!-- col-6 -->
    <div class="col-sm-8 col-md-6 col-xl-4">
        <div class="az-content-label az-content-label-sm mg-b-15">Ofis Verileri</div>
        <div class="ht-200 ht-lg-250"><canvas id="chartBar2"></canvas></div>
        <div id="chart-tooltip"></div>
    </div>

    <div class="col-md-4">
        <div class="col-md">
            <input id="nameInput" class="form-control" placeholder="Input box" type="text">
            <br>
            <div class="input-group">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        Date:
                    </div>
                </div>
                <input id="dateInput" type="text" class="form-control" placeholder="Ay Giriniz...">
                <div class="col-sm-6 col-md-4"><button id="submitButton" class="btn btn-indigo btn-rounded btn-block">Göster</button></div>
            </div>
        </div><!-- input-group -->
    </div>
</div><!-- row -->

